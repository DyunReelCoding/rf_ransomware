{
  "manifest_version": 3,
  "name": "Ransomware Detector",
  "version": "1.0",
  "permissions": [
    "downloads",
    "notifications",
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["warning.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
